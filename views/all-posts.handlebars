<div class="posts">
  {{#each posts}}
  <article class="card">
    <div class="card-header">
      <h2>
        <a href="/post/{{id}}">{{title}}</a>
      </h2>
      <div>
        Posted by {{User.username}} on {{format_date createdAt}}
        <button class = "btn"><a class="link" href="/edit/{{post.id}}"></a>Edit Post</button>
        
        
      </div>
    </div>
    <div class="card-body">
      {{body}}
    </div>
  </article>
  {{/each}}
</div>
<script src = "/js/edit.js"></script>